#include <iostream>
using namespace std;

int main(){
    int num, rem, i = 0, k, sum = 0, p = 100;
    cin >> num;
    while(num != 0){
        rem = num%10;
        sum = sum + rem*p;
        num = num/10;
        i++;
        p /= 10;
    }
    cout << sum;
    
    return 0;
}

output/-
123
321
