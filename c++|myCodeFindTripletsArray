#include <iostream>
using namespace std;

int main(){
    int arr[7] = {1,2,3,4,5,6,7},i , j, k, triplet = 9, sum;
    for(i = 0; i < 7; i++){
        for(j = i + 1; j < 7; j++){
            for(k = j + 1; k < 7; k++){
             if(arr[i] + arr[j] + arr[k] == triplet){
                cout << arr[i] << " " << arr[j] << " " << arr[k]<< endl;   
            }
            
            }
        }
    }
    
    
    return 0;
}

output/-
[?2004l
1 2 6
1 3 5
2 3 4
[?2004h
